{
  "name": "node-supervisord",
  "version": "1.0.3",
  "description": "A Node.js library for communicating with the Supervisord XML-RPC API",
  "keywords": [
    "supervisor",
    "supervisord",
    "sdk",
    "typescript",
    "module",
    "addon"
  ],
  "homepage": "https://github.com/bramanda48/node-supervisord",
  "repository": "git@github.com:bramanda48/node-supervisord.git",
  "author": {
    "name": "bramanda48",
    "email": "bram@malang.dev",
    "url": "https://malang.dev/"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "format": "prettier --write .",
    "build": "tsc -b tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/xmlrpc": "^1.3.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.2.3"
  },
  "lint-staged": {
    "src/**/*.{ts|test.ts}": [
      "eslint --max-warnings 0 \"src/**/*.ts\"",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "url": "^0.11.3",
    "xmlrpc": "^1.3.2"
  },
  "engines": {
    "node": ">= 18"
  }
}
